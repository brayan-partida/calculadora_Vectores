<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_bright"
    android:orientation="vertical"

    tools:context=".MainActivity">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/prod_cruz"
                android:layout_width="257dp"
                android:layout_height="78dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="624dp"
                android:layout_marginEnd="8dp"
                android:background="@android:color/holo_green_dark"
                android:onClick="Onclick"

                android:text="Aplicar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.467"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/valor1"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:ems="10"

                android:text="A1"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="72dp" />

            <EditText
                android:id="@+id/A1"
                android:layout_width="107dp"
                android:layout_height="52dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="276dp"
                android:layout_marginBottom="8dp"
                android:ems="10"
                android:hint="A1"
                android:inputType="numberSigned|numberDecimal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.282"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/valor2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:ems="10"

                android:text="A2"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="72dp" />

            <EditText
                android:id="@+id/A2"
                android:layout_width="113dp"
                android:layout_height="46dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="136dp"
                android:layout_marginBottom="8dp"
                android:ems="10"
                android:hint="A2"
                android:inputType="numberSigned|numberDecimal"
                app:layout_constraintBottom_toBottomOf="@+id/prod_cruz"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.827"
                app:layout_constraintStart_toEndOf="@+id/A1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.286"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/valor3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:ems="10"

                android:text="A3"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="72dp" />


            <EditText
                android:id="@+id/A3"
                android:layout_width="110dp"
                android:layout_height="49dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:ems="10"
                android:hint="A3"
                android:inputType="numberSigned|numberDecimal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.296"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/valor5"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:ems="10"

                android:text="B1"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="72dp" />

            <EditText
                android:id="@+id/B1"
                android:layout_width="83dp"
                android:layout_height="42dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="344dp"
                android:ems="10"
                android:hint="B1"
                android:inputType="numberSigned|numberDecimal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/B3"
                app:layout_constraintHorizontal_bias="0.027"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/valor4"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:ems="10"

                android:text="B2"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="72dp" />

            <EditText
                android:id="@+id/B2"
                android:layout_width="92dp"
                android:layout_height="54dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="340dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:hint="B2"
                android:inputType="numberSigned|numberDecimal"
                app:layout_constraintBottom_toBottomOf="@+id/prod_cruz"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.554"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/valor6"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:ems="10"

                android:text="B3"

                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="72dp" />

            <EditText
                android:id="@+id/B3"
                android:layout_width="90dp"
                android:layout_height="49dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:ems="10"
                android:hint="B3"
                android:inputType="numberSigned|numberDecimal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.973"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.51"
                tools:ignore="MissingConstraints" />


            <Spinner
                android:id="@+id/seleccion"
                android:layout_width="308dp"
                android:layout_height="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="132dp"
                app:layout_constraintBottom_toTopOf="@+id/prod_cruz"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.536"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.922"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/escalar_a"
                android:layout_width="124dp"
                android:layout_height="45dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="44dp"
                android:ems="10"
                android:hint="escalar A"
                android:inputType="numberDecimal|numberSigned"
                app:layout_constraintBottom_toTopOf="@+id/prod_cruz"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.095"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seleccion"
                app:layout_constraintVertical_bias="0.605" />

            <EditText
                android:id="@+id/escalar_b"
                android:layout_width="140dp"
                android:layout_height="47dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="72dp"
                android:ems="10"
                android:hint="Escalar b"
                android:inputType="numberDecimal|numberSigned"
                app:layout_constraintBottom_toTopOf="@+id/prod_cruz"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.782"
                app:layout_constraintStart_toEndOf="@+id/escalar_a"
                app:layout_constraintTop_toBottomOf="@+id/seleccion"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textoEscalarA"
                android:layout_width="96dp"
                android:layout_height="24dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="264dp"
                android:layout_marginBottom="176dp"
                android:text="Escalar de A"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.939" />

            <TextView
                android:id="@+id/textoEscalarB"
                android:layout_width="91dp"
                android:layout_height="24dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:text="Escalar de B"
                app:layout_constraintBottom_toTopOf="@+id/escalar_b"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.777"
                app:layout_constraintStart_toEndOf="@+id/textoEscalarA"
                app:layout_constraintTop_toBottomOf="@+id/seleccion"
                app:layout_constraintVertical_bias="0.47" />

            <Spinner
                android:id="@+id/spinner"
                android:layout_width="290dp"
                android:layout_height="35dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="608dp"
                app:layout_constraintBottom_toBottomOf="@+id/prod_cruz"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.457"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>